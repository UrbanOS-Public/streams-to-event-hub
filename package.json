{
    "name": "streams-to-event-hub",
    "version": "0.0.1",
    "description": "Forward events from a websocket to event hub",
    "main": "dist/index.js",
    "scripts": {
        "start": "node dist/index.js",
        "build": "tsc",
        "watch": "nodemon src/index.ts",
        "test": "jest --coverage",
        "view:cov": "serve coverage/lcov-report",
        "clean": "rm -rf coverage dist",
        "lint": "eslint 'src/**/*.ts'",
        "lint:fix": "eslint 'src/**/*.ts' --fix",
        "test:watch": "jest --coverage --watchAll",
        "detect": "jest --runInBand --detectOpenHandles"
    },
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@azure/event-hubs": "^5.6.0",
        "@types/jest": "^25.2.3",
        "@types/node": "^14.0.11",
        "@types/source-map-support": "^0.5.1",
        "@types/ws": "^8.2.0",
        "@typescript-eslint/eslint-plugin": "^3.1.0",
        "@typescript-eslint/parser": "^3.1.0",
        "eslint": "^7.2.0",
        "eslint-config-prettier": "^6.11.0",
        "eslint-plugin-prettier": "^3.1.3",
        "jest": "^26.0.1",
        "onchange": "^7.0.2",
        "prettier": "^2.0.5",
        "serve": "^11.3.2",
        "source-map-support": "^0.5.19",
        "ts-jest": "^26.1.0",
        "ts-node": "^8.10.2",
        "typescript": "^3.9.5",
        "ws": "^8.2.3"
    },
    "devDependencies": {
        "jest-junit": "^13.0.0",
        "mocked-env": "^1.3.5",
        "nodemon": "^2.0.13",
        "wait-for-expect": "^3.0.2"
    }
}
